patterns
n=3
for i in range(0,3):
  for j in range(n-i):
    print("*",end='')
  print()


n=3
for i in range(0,3):

  for j in range(n-i):
    print(" ",end='')

  for j in range(i+1):
    print("*",end='')
  print()


for i in range(0,3):
  for j in range(n-i):
    print(" ",end='')
  for j in range(i):
    print("*", end='')
  for j in range(i+1):
    print("*",end='')
  print()



