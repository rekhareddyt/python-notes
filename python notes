n=11
c=1
for i in range(2,n):
  if(n%i==0):
    c=2
    break
if(c==1):
  print("Prime")
else:
  print("Not Prime")

def prime(n):
  if(n<2):
    print("Dont Enter like CSE STUDENTS")
    return 0
  c=True
  for i in range(2,n):
    if(n%i==0):
      c=False
      break
  return c
c=prime(1)
if(c==0):
  pass
elif(c):
  print("Prime")
else:
  print("Not Prime")


a=[]
def fib(n):

  if(n==1):
    return 0
    a[1]=0
  elif(n==2):
    return 1
    a[2]=1
  else:
    return fib(n-1)+fib(n-2)

n=20
for i in range(1,21):
  print(fib(i),end=' ')

def fact(n):
  if(n==0 or n==1):
    return 1
  else:
    return n*fact(n-1)
print(fact(5))

